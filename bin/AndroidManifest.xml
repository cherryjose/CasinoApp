    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainActivity" >
        </activity>
        <activity android:name=".RegisterationActivity" >
        </activity>
        <activity
            android:name=".Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AllGames"
            android:label="@string/title_activity_all_games" >
        </activity>
        <activity android:name=".PlayMusic" >
        </activity>
        <activity android:name=".NormalActivity" >
        </activity>
        <activity android:name=".MyActivity" >
        </activity>
        <activity
            android:name=".ViewResult"
            android:label="@string/title_activity_view_result" >
        </activity>
    </application>

</manifest>
