<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="0dp"
    android:background="@color/background"
    android:gravity="center_horizontal"
    android:label="@string/normal_activity_label"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".NormalActivity" >

    <TextView
        android:id="@+id/header"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dip"
        android:text="           Tic Tac Toe"
        android:textColor="#7f345a"
        android:textSize="32dp"
        android:textStyle="bold"
        android:typeface="serif" >

</TextView>

    <Button
        android:id="@+id/btn00"
        android:layout_width="75px"
        android:layout_height="75px"
        android:layout_below="@+id/header"
        android:layout_marginBottom="10dip"
        android:layout_marginLeft="70dip"
        android:layout_marginRight="10dip"
        android:layout_marginTop="20dip"
        android:text=""
        android:textSize="25sp" >
</Button>

    <Button
        android:id="@+id/btn01"
        android:layout_width="75px"
        android:layout_height="75px"
        android:layout_below="@+id/header"
        android:layout_marginBottom="10dip"
        android:layout_marginLeft="10dip"
        android:layout_marginRight="10dip"
        android:layout_marginTop="20dip"
        android:layout_toRightOf="@+id/btn00"
        android:text=""
        android:textSize="25sp" >
</Button>

    <Button
        android:id="@+id/btn02"
        android:layout_width="75px"
        android:layout_height="75px"
        android:layout_below="@+id/header"
        android:layout_marginBottom="10dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="20dip"
        android:layout_toRightOf="@+id/btn01"
        android:text=""
        android:textSize="25sp" >
</Button>/

     <Button
         android:id="@+id/btn10"
         android:layout_width="75px"
         android:layout_height="75px"
         android:layout_below="@+id/btn00"
         android:layout_marginBottom="10dip"
         android:layout_marginLeft="70dip"
         android:layout_marginRight="10dip"
         android:layout_marginTop="10dip"
         android:text=""
         android:textSize="25sp" >
</Button>"

      <Button
          android:id="@+id/btn11"
          android:layout_width="75px"
          android:layout_height="75px"
          android:layout_below="@+id/btn01"
          android:layout_marginBottom="10dip"
          android:layout_marginLeft="10dip"
          android:layout_marginRight="10dip"
          android:layout_marginTop="10dip"
          android:layout_toRightOf="@+id/btn10"
          android:text=""
          android:textSize="25sp" >
</Button>

      <Button
          android:id="@+id/btn12"
          android:layout_width="75px"
          android:layout_height="75px"
          android:layout_below="@+id/btn02"
          android:layout_marginBottom="10dip"
          android:layout_marginLeft="10dip"
          android:layout_marginTop="10dip"
          android:layout_toRightOf="@+id/btn11"
          android:text=""
          android:textSize="25sp" >
</Button>

      <Button
          android:id="@+id/btn20"
          android:layout_width="75px"
          android:layout_height="75px"
          android:layout_below="@+id/btn10"
          android:layout_marginBottom="10dip"
          android:layout_marginLeft="70dip"
          android:layout_marginRight="10dip"
          android:layout_marginTop="10dip"
          android:text=""
          android:textSize="25sp" >
</Button>

      <Button
          android:id="@+id/btn21"
          android:layout_width="75px"
          android:layout_height="75px"
          android:layout_below="@+id/btn11"
          android:layout_marginBottom="10dip"
          android:layout_marginLeft="10dip"
          android:layout_marginRight="10dip"
          android:layout_marginTop="10dip"
          android:layout_toRightOf="@+id/btn20"
          android:text=""
          android:textSize="25sp" >
</Button>

      <Button
          android:id="@+id/btn22"
          android:layout_width="75px"
          android:layout_height="75px"
          android:layout_below="@+id/btn12"
          android:layout_marginBottom="10dip"
          android:layout_marginLeft="10dip"
          android:layout_marginTop="10dip"
          android:layout_toRightOf="@+id/btn21"
          android:text=""
          android:textSize="25sp" >
</Button>

      <TextView
          android:id="@+id/txtLbl"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/btn20"
          android:layout_marginBottom="10dip"
          android:layout_marginLeft="70dip"
          android:layout_marginRight="10dip"
          android:layout_marginTop="20dip"
          android:text="Playing"
          android:textColor="#7f345a"
          android:textSize="20dp"
        android:textStyle="bold"
        android:typeface="serif"  >

</TextView>

      <TextView
          android:id="@+id/playerLbl"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_below="@+id/btn21"
          android:layout_marginBottom="10dip"
          android:layout_marginLeft="10dip"
          android:layout_marginRight="10dip"
          android:layout_marginTop="20dip"
          android:layout_toRightOf="@+id/txtLbl"
          android:text="Player1"
          android:textColor="#7f345a"
          android:textSize="20dp"
        android:textStyle="bold"
        android:typeface="serif"  >

</TextView>

    <View
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="7dp"
            android:layout_alignLeft="@+id/imageView1"
            android:layout_alignParentBottom="true"
            android:layout_alignRight="@+id/imageView1"
            android:layout_alignTop="@+id/C"
            android:layout_marginBottom="49dp"
            android:background="#990000" />
        
        <TextView
	    android:id="@+id/TextView01"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignTop="@+id/view1"
	    android:layout_centerHorizontal="true"
	    android:layout_marginTop="17dp"
	    android:text="@string/copyright"
	    android:textColor="#990000"
	    android:textAppearance="?android:attr/textAppearanceSmall" />
</RelativeLayout>